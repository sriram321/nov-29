sno=int(input("Enter student number"))
sname=input("Enter student name:")
group=input("Enter group:")
maths=int(input("enter marks scored in maths:"))
english=int(input("enter marks scored in english:"))
AFM=int(input("enter marks scored in AFM:"))
tm=maths+english+AFM
avg=tm/3
if avg>90:
        print("distinction")
elif avg>75:
    print("first class")
elif avg>60:
    print("second class")
elif avg>50:
    print("third class")
elif avg>35:
    print("pass")
else:
    print("fail")
    
